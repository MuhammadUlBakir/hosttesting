{
  "builds": [
    {
      "src": "/api/.*",
      "use": "@vercel/node"
    },
    {
      "src": "/client/.*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/images/(.*)",
      "dest": "/api/images/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ]
}
